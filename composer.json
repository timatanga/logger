{
    "name": "timatanga/logger",
    "description": "The timatanga Log package.",
    "type": "library",
    "require": {
        "php": "^8.0",
        "monolog/monolog": "2.x-dev",
        "psr/log": "3.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev",
        "symfony/var-dumper": "6.0.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "timatanga\\Logger\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Mark Fluehmann",
            "email": "dbiz.apps@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "timatanga\\Logger\\Providers\\LoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
